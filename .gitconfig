[core]
    pager = less -SFXni -x4
    editor = vim
    autocrlf = false
    ignorecase = false
    quotepath = false
[pull]
    rebase = false
[fetch]
    prune = false
[rebase]
    autoStash = false
[alias]
    a = "!f(){ git add $@ && git status; }; f"
    aa = !git add -A && git status
    al = !git config --global --list | grep alias | sort
    au = !git add -u && git status
    b = branch
    bu = !git branch --unset-upstream && git status
    ca = !git commit --amend --date=\"$(date -R)\"
    cg = config --global
    cm = commit
    co = checkout
    cp = cherry-pick
    d = diff
    dc = diff --cached
    f = fetch -p origin
    ignore = !git rm -r --cached . && git add -A
    ll = log --oneline --graph --decorate=full
    lla = log --oneline --graph --decorate=full --all
    pf = push -f
    po = !git push -u origin $(git rev-parse --abbrev-ref @)
    rb = rebase
    rh1 = reset HEAD~
    rs = "!f(){ git restore $@ && git status; }; f"
    rss = "!f(){ git restore -S $@ && git status; }; f"
    s = status
    sd = stash drop
    sl = stash list
    sp = stash pop
    ss = stash push
    sw = switch
